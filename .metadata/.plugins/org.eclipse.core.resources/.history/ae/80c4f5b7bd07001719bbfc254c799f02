package Test0310;
//short s; byte b; char c; 则表达式s * b + c
public class Test {    
String str = new String("Hi !");   
char[] ch = { 'L', 'i', 'k', 'e' };    
public static void main(String args[]) {    
Test ex = new Test();    
ex.change(ex.str, ex.ch);     
System.out.print(ex.str + " ");    
System.out.print(ex.ch);    }    
public void change(String str, char ch[]) {   
 str = "How are you";    
ch[1] = 'u';    }  
}  




