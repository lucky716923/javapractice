package Test0308;
	//向上抽取的父类
public class Pet {
	//名字
	private String name;
	//毛色
	private String color;
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getColor() {
		return color;
	}
	public void setColor(String color) {
		this.color = color;
	}
	//吃饭
	public void eat(){
		
	}
	//睡觉
	public void sleep(){
		
	}
	
}
