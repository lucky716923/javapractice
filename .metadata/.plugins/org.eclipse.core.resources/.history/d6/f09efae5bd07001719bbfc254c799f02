package Test0310;
//short s; byte b; char c; 则表达式s * b + c

public class Test {
    int count = 9;
    public void count() {
    	count++;
        System.out.println("count=" + count);
    }
    public static void main(String args[]) {
        new Test().count();
        new Test().count();
    }
}





