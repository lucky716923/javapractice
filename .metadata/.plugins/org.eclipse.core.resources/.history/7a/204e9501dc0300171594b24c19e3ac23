package Test0308;

public class TicketShop implements Runnable{
	private String startName;//出发地
	private String sotpName;//目的地
	private boolean flag=true;//没有票
	
	public String getStartName() {
		return startName;
	}

	public void setStartName(String startName) {
		this.startName = startName;
	}

	public String getSotpName() {
		return sotpName;
	}

	public void setSotpName(String sotpName) {
		this.sotpName = sotpName;
	}

	@Override
	public void run() {
		// TODO Auto-generated method stub
		if(!flag){
			try {
				super.wait();
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		}
		
	}
	
	

}
